{
  "name": "nagular",
  "displayName": "nagular",
  "description": "Quick navigate on angular",
  "version": "0.0.1",
  "publisher": "nagular",
  "engines": {
    "vscode": "^1.79.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tardisbleu/vscode-nagular.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "Nagular",
        "properties": {
          "nagular.actionButton": {
            "title": "Show icon to navigate in action bar",
            "type": "boolean",
            "default": true,
            "description": "Show icon in action bar"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "nagular.goToTs",
        "title": "Nagular : Go to TS",
        "icon": "$(file-code)"
      },
      {
        "command": "nagular.goToHtml",
        "title": "Nagular : Go to Html",
        "icon": "$(code)"
      },
      {
        "command": "nagular.goToStyle",
        "title": "Nagular : Go to Style",
        "icon": "$(symbol-color)"
      },
      {
        "command": "nagular.goToTest",
        "title": "Nagular : Go to Test",
        "icon": "$(beaker)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "config.nagular.actionButton && nagular.showActionButton",
          "command": "nagular.goToTs",
          "group": "navigation@1"
        },
        {
          "when": "config.nagular.actionButton && nagular.showActionButton",
          "command": "nagular.goToHtml",
          "group": "navigation@2"
        },
        {
          "when": "config.nagular.actionButton && nagular.showActionButton",
          "command": "nagular.goToStyle",
          "group": "navigation@3"
        },
        {
          "when": "config.nagular.actionButton && nagular.showActionButton",
          "command": "nagular.goToTest",
          "group": "navigation@4"
        }
      ]
    },
    "keybindings": [
      {
        "command": "nagular.goToTs",
        "key": "ctrl+F1",
        "mac": "cmd+F1"
      },
      {
        "command": "nagular.goToHtml",
        "key": "ctrl+F2",
        "mac": "cmd+F2"
      },
      {
        "command": "nagular.goToStyle",
        "key": "ctrl+F3",
        "mac": "cmd+F3"
      },
      {
        "command": "nagular.goToTest",
        "key": "ctrl+F4",
        "mac": "cmd+F4"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  }
}
